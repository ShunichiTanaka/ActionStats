p#notice = notice

table
  tbody
    tr
      td = t('activerecord.attributes.outcome.category')
      td = @outcome.category&.name
    tr
      td = t('activerecord.attributes.outcome.name')
      td = @outcome.name
    tr
      td = t('activerecord.attributes.outcome.display_order')
      td = @outcome.display_order
    tr
      td = t('activerecord.attributes.outcome.published')
      - published_mark = @outcome.published ? 'check_mark.png' : 'uncheck_mark.png'
      td = image_tag published_mark, class: 'publish-mark'

= link_to '編集', edit_service_outcome_path(@outcome)
